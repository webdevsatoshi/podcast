    "express": "^4.18.2",
    "puppeteer": "^24.36.1",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/puppeteer": "^5.4.7",
    "@types/three": "^0.160.0",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
